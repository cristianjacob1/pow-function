import java.util.*;

public class Main {
    
         static void pow(double x,int n){
            var res=x;
            for(int i= Math.abs(n); i>1;i--){
                res=res*x;
            }
            if(n<0){
                res=1/res;
            }
            System.out.println(res);
        }
    public static void main(String[] args) throws Exception {
        
        System.out.println("XXXXXXXX");
        pow(2.00000, -2);
    }
}
